import java.util.*;
import java.lang.*;

class Main{
    
    public static void main(String[] args){
        
        Scanner ob=new Scanner(System.in);
        int a=ob.nextInt();
        int b=ob.nextInt();
        int c=ob.nextInt();
        int avg=(a+b+c)/3;
        System.out.println("average:"+avg);
        
    }
}
