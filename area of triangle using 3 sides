import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner ob=new Scanner(System.in);
		int a=ob.nextInt();
		int b=ob.nextInt();
		int c=ob.nextInt();
		float s=(a+b+c)/2f;
		double area=Math.sqrt(s*(s-a)*(s-b)*(s-c));
		System.out.println("area of triangle with 3 sides:"+area);
	}
}
