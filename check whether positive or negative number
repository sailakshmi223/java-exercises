import java.util.*;
import java.lang.*;

class Main{
    public static void main(String[] args){
        Scanner ob=new Scanner(System.in);
        int n=ob.nextInt();
        if(n>0){
            System.out.println("positive number");
        }
        else if(n<0){
            System.out.println("negative number");
        }
        else{
            System.out.println("neutral");
        }
    }
}
