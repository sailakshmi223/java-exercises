import java.util.*;
import java.lang.*;

class Main{
    public static void main(String[] args){
        Scanner ob=new Scanner(System.in);
        int n=ob.nextInt();
        boolean prime=true;
        for(int i=2;i<=n-1;i++){
            if(n%2==0){
                System.out.println("not a prime number");
                prime=false;
                break;
            }
            if(prime==true){
                System.out.println("prime number");
            }
        }
    }
}
