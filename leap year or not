import java.util.*;
import java.lang.*;

class Main{
    public static void main(String[] args){
        Scanner ob=new Scanner(System.in);
        int year=ob.nextInt();
        if((year%4==0)&&(year%100!=0)){
            System.out.println("leapyear");
        }
        else if(year%400==0){
            System.out.println("leapyear ");
        }
        else{
            System.out.println("not a leapyear");
        }
    }
}
